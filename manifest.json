{
    "manifest_version": 2,
    "name": "Password Manager",
    "version": "1.0",
    "description": "A password generator and manager extension",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": ["storage"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "manifest_key": "value"
  }
  

